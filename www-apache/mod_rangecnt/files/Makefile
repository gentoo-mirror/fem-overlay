##
##  Makefile -- Build procedure for sample limitipconn Apache module
##  Autogenerated via ``apxs -n rangecnt -g''.
##

#   the used tools
APXS=apxs
APACHECTL=apachectl

#   additional user defines, includes and libraries
#DEF=-Dmy_define=my_value
#INC=-Imy/include/dir
#LIB=-Lmy/lib/dir -lmylib

#   the default target
all: mod_rangecnt.so

#   compile the DSO file
mod_rangecnt.so: mod_rangecnt.c
	$(APXS) -c $(DEF) $(INC) $(LIB) mod_rangecnt.c

#   install the DSO file into the Apache installation
#   and activate it in the Apache configuration
install: all
	$(APXS) -c -i -a -n 'rangecnt' mod_rangecnt.c

#   cleanup
clean:
	-rm -rf mod_rangecnt.o mod_rangecnt.so mod_rangecnt.lo mod_rangecnt.slo mod_rangecnt.la .libs  

